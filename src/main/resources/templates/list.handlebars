    <script type="text/javascript">
    $(function () {
        $("#table_cell").click(function () {
            if ($(this).is(":checked")) {
              $("#default_admin_page_for_students").hide();
               $("#admin_page_for_students_after_search").show();
            }else {
                 $("#admin_page_for_students_after_search").hide();
                  $("#default_admin_page_for_students").show();
            }

        });

    });


  </script>

    <script type="text/javascript">
    $(function () {
        $("#table_cell_back").click(function () {
            if ($(this).is(":checked")) {
              $("#default_admin_page_for_students").show();
               $("#admin_page_for_students_after_search").hide();

            }else {
                 $("#admin_page_for_students_after_search").hide();
